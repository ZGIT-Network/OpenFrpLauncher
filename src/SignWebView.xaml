<Window
    x:Class="OpenFrp.Launcher.SignWebView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:OpenFrp.Launcher"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:ui="http://schemas.modernwpf.com/2019"
    xmlns:webView="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
    Title="SignWebView"
    Width="300"
    Height="350"
    ui:ThemeManager.RequestedTheme="{Binding MainWindow.(ui:ThemeManager.RequestedTheme), Source={x:Static local:App.Current}, Mode=OneWay}"
    ui:WindowHelper.UseModernWindowStyle="True"
    WindowStyle="ToolWindow"
    mc:Ignorable="d">
