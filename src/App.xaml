<Application
    x:Class="OpenFrp.Launcher.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:pf="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    xmlns:local="clr-namespace:OpenFrp.Launcher">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Awe.UI;component/Theme.xaml" />

            </ResourceDictionary.MergedDictionaries>
            <FontFamily x:Key="Montserrat">
                Montserrat
            </FontFamily>
            <CubicEase x:Key="EasingFunc" EasingMode="EaseOut" pf:Freeze="True"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
