	  <None Remove="Resources\Montserrat.ttf" />
	  <None Remove="Resources\wallhaven_9bd9b167ly1fwsfjk5xlxj21hc0u0qu9.jpg" />
	</ItemGroup>



	<ItemGroup>
	  <ProjectReference Include="..\..\Awe.Model\Awe.Model.csproj" />
